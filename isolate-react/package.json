{
  "name": "isolate-react",
  "version": "2.0.3",
  "repository": "https://github.com/davidmfoley/isolate-react",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "davidmfoley@gmail.com",
  "license": "MIT",
  "private": false,
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rm -rf ./lib/",
    "test": "mocha",
    "test:watch": "nodemon -q -e ts,tsx --exec \"yarn test\" --watch src --watch test",
    "prettier": "prettier -c '{src,test}/**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "cover": "COVERAGE=1 nyc mocha && open coverage/index.html",
    "ci": "yarn build:tsc && yarn test && yarn prettier"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/react": "16.9.12",
    "chai": "^4.2.0",
    "core-js": "^3.8.3",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-router-dom": "^5.3.0",
    "regenerator-runtime": "^0.13.7",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.3.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "mocha": {
    "extension": [
      "ts",
      "tsx"
    ],
    "require": [
      "test/babel-register.js"
    ],
    "watch-files": [
      "./src/**/*",
      "./test/**/*"
    ],
    "reporter": "dot",
    "recursive": true
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
